generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Lead {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  name      String
  email     String
  message   String
  source    String?
  service   String?
}
